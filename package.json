{
  "name": "typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node ./src/main.ts",
    "build": "tsc",
    "start": "node ./dist/main.js",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.1.61",
    "@swc/core": "^1.3.35",
    "@types/jest": "^29.5.5",
    "@types/node": "^18.13.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "jest": {
    "verbose": true,
    "clearMocks": false,
    "collectCoverage": false,
    "moduleFileExtensions": [
      "js",
      "ts",
      "node"
    ],
    "resetMocks": true,
    "resetModules": true,
    "restoreMocks": true,
    "testMatch": [
      "**/*.test.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/coverage",
      "/dist/"
    ],
    "transform": {
      "\\.ts?$": "ts-jest"
    }
  }
